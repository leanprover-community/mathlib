{
    "problemMatcher": [
        {
            "owner": "lean-build",
            "pattern": [
                {
                    "regexp": "^(.*?):(.*?):(.*?): (.*?): ([\\S\\s]*?)\n\n",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            ]
        }
    ]
}
